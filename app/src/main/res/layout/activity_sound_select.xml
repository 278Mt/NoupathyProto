<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SoundSelectActivity" android:background="@color/peachy_pink">

    <TextView
            android:text="音を変更する"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:textSize="26dp"
            android:textColor="@android:color/white" android:fontFamily="@font/noto_sans_bold"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="704dp"
            android:layout_height="142dp" app:srcCompat="@drawable/snd_bg_img"
            android:id="@+id/imageView27" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="704dp"
            android:layout_height="142dp" app:srcCompat="@drawable/snd_bg_img"
            android:id="@+id/imageView28" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView27" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="704dp"
            android:layout_height="142dp" app:srcCompat="@drawable/snd_bg_img"
            android:id="@+id/imageView29" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView28" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="音のセット1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="19sp"
            android:textColor="@color/peachy_pink" android:fontFamily="@font/noto_sans"
            android:layout_marginTop="23dp"
            app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintStart_toStartOf="@+id/use1" app:layout_constraintEnd_toEndOf="@+id/use1"/>
    <TextView
            android:text="音のセット2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:textSize="19sp"
            android:textColor="@color/peachy_pink" android:fontFamily="@font/noto_sans"
            app:layout_constraintTop_toTopOf="@+id/imageView28"
            android:layout_marginTop="23dp"
            app:layout_constraintStart_toStartOf="@+id/use2" app:layout_constraintEnd_toEndOf="@+id/use2"/>
    <TextView
            android:text="音のセット3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" android:textSize="19sp"
            android:textColor="@color/peachy_pink" android:fontFamily="@font/noto_sans"
            app:layout_constraintTop_toTopOf="@+id/imageView29"
            android:layout_marginTop="23dp"
            app:layout_constraintStart_toStartOf="@+id/use3" app:layout_constraintEnd_toEndOf="@+id/use3"/>
    <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:id="@+id/view6" android:background="@color/peachy_pink"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginTop="3dp"
            app:layout_constraintStart_toStartOf="@+id/use2" app:layout_constraintEnd_toEndOf="@+id/use2"/>
    <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:id="@+id/view7" android:background="@color/peachy_pink"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="3dp"
            app:layout_constraintStart_toStartOf="@+id/use3" app:layout_constraintEnd_toEndOf="@+id/use3"/>
    <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:id="@+id/view8" android:background="@color/peachy_pink"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:layout_marginTop="3dp"
            app:layout_constraintStart_toStartOf="@+id/use1" app:layout_constraintEnd_toEndOf="@+id/use1"/>
    <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:id="@+id/use1"
            android:background="@drawable/snd_use_btn_set" app:layout_constraintStart_toStartOf="@+id/imageView27"
            android:layout_marginStart="21dp" android:layout_marginBottom="27dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27"/>
    <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:id="@+id/use2"
            android:background="@drawable/snd_use_btn_set"
            android:layout_marginBottom="27dp" app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintStart_toStartOf="@+id/imageView28" android:layout_marginStart="21dp"/>
    <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:id="@+id/use3"
            android:background="@drawable/snd_use_btn_set"
            android:layout_marginBottom="27dp" app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintStart_toStartOf="@+id/imageView29" android:layout_marginStart="21dp"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound1_1"
            app:layout_constraintBottom_toBottomOf="@+id/use1" app:layout_constraintStart_toEndOf="@+id/use1"
            android:layout_marginStart="28dp" android:background="@drawable/snd_s1_1_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound1_3"
            app:layout_constraintBottom_toBottomOf="@+id/sound1_2" app:layout_constraintStart_toEndOf="@+id/sound1_2"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s1_3_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound1_2"
            app:layout_constraintBottom_toBottomOf="@+id/sound1_1" app:layout_constraintStart_toEndOf="@+id/sound1_1"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s1_2_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound1_5"
            app:layout_constraintBottom_toBottomOf="@+id/sound1_4" app:layout_constraintStart_toEndOf="@+id/sound1_4"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s1_5_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound2_1"
            app:layout_constraintBottom_toBottomOf="@+id/use2" app:layout_constraintStart_toEndOf="@+id/use2"
            android:layout_marginStart="28dp" android:background="@drawable/snd_s2_1_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound1_4"
            app:layout_constraintBottom_toBottomOf="@+id/sound1_3" app:layout_constraintStart_toEndOf="@+id/sound1_3"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s1_4_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound2_4"
            app:layout_constraintBottom_toBottomOf="@+id/sound2_3" app:layout_constraintStart_toEndOf="@+id/sound2_3"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s2_4_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound2_5"
            app:layout_constraintBottom_toBottomOf="@+id/sound2_4" app:layout_constraintStart_toEndOf="@+id/sound2_4"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s2_5_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound2_2"
            app:layout_constraintBottom_toBottomOf="@+id/sound2_1" app:layout_constraintStart_toEndOf="@+id/sound2_1"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s2_2_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound3_1"
            app:layout_constraintBottom_toBottomOf="@+id/use3" app:layout_constraintStart_toEndOf="@+id/use3"
            android:layout_marginStart="28dp" android:background="@drawable/snd_s3_1_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound3_2"
            app:layout_constraintBottom_toBottomOf="@+id/sound3_1" app:layout_constraintStart_toEndOf="@+id/sound3_1"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s3_2_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound3_3"
            app:layout_constraintBottom_toBottomOf="@+id/sound3_2" app:layout_constraintStart_toEndOf="@+id/sound3_2"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s3_3_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound2_3"
            app:layout_constraintBottom_toBottomOf="@+id/sound2_2" app:layout_constraintStart_toEndOf="@+id/sound2_2"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s2_3_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound3_5"
            app:layout_constraintBottom_toBottomOf="@+id/sound3_4" app:layout_constraintStart_toEndOf="@+id/sound3_4"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s3_5_img_set"/>
    <Button
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:id="@+id/sound3_4"
            app:layout_constraintBottom_toBottomOf="@+id/sound3_3" app:layout_constraintStart_toEndOf="@+id/sound3_3"
            android:layout_marginStart="16dp" android:background="@drawable/snd_s3_4_img_set"/>
</android.support.constraint.ConstraintLayout>